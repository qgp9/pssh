## GROUP: Cloudflare_Tunnel
Host vps1.c
  HostName vps1-c.example.com
  IdentityFile ~/.ssh/pkey/vultr_id_rsa
  User ubuntu

Host vps2.c
  HostName vps2-c.example.com
  IdentityFile ~/.ssh/pkey/vultr_id_rsa
  User root

Host *.c
  ProxyCommand cloudflared access ssh -T %h


## GROUP: Default
Host *
  Protocol 2
  IdentityFile ~/.ssh/id_rsa
  StrictHostKeyChecking no
  ForwardAgent yes
  ControlMaster auto
  ControlPath ~/.ssh/controlmasters/%r@%h:%p
  ControlPersist 1h
  ForwardAgent yes


